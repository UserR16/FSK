
local repo = 'https://raw.githubusercontent.com/mstudio45/LinoriaLib/main/'
local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))()
local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))()
local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()

local Options = Library.Options
local Toggles = Library.Toggles

Library.ShowToggleFrameInKeybinds = true
Library.ShowCustomCursor = true
Library.NotifySide = "Right"

local Window = Library:CreateWindow({
	Title = '[v0.2] Forsaken',
	Center = true,
	AutoShow = true,
	Resizable = true,
	ShowCustomCursor = true,
	NotifySide = "Right",
	TabPadding = 8,
	MenuFadeTime = 0
})

local Tabs = {
  Info = Window:AddTab('Info'),
  Anti = Window:AddTab('Anti'),
  Visual = Window:AddTab('Visual'),
  Auto = Window:AddTab('Auto'),
  Generators = Window:AddTab('Generator'),
  Settings = Window:AddTab('Setting')
}

local Info = Tabs.Info:AddLeftGroupbox("Info")
local username = "coming soon!"
Info:AddLabel('Username : ' .. username)
local userid = "coming soon!"
Info:AddLabel('User ID : ' .. userid)
local client = "coming soon!"
Info:AddLabel('Client : ' .. client)
local device = "coming soon!"
Info:AddLabel('Device : ' .. device)
local Anti = Tabs.Anti:AddLeftGroupo("Anti")
Anti:AddToggle("Anti Hallucination",{
Text = "Anti Fake Noli",
Default = false,
Tooltip = "This is features remove fake noli.",
Callback = function(v)
if v and game:GetService("ReplicatedStorage").Modules.StatusEffects.KillerExclusive:FindFirstChild("Hallucination") then
game:GetService("ReplicatedStorage").Modules.StatusEffects.KillerExclusive.Hallucination.Parent = workspace
else
if workspace:FindFirstChild("Hallucination") then
workspace.Hallucination.Parent = game:GetService("ReplicatedStorage").Modules.StatusEffects.KillerExclusive
end
end})
Anti:AddToggle("Anti John Doe Trail",{
Text = "Anti John Trail",
Default = false,
Tooltip = "This is features help you cant touch with 'trail' john doe.",
Callback = function(v)
if v then
for _,v in ipairs(workspace.Players.Killers:GetDescendants()) do
if v.Name == "Trail" then
v.CanTouch = false
end
end
AntiJohnDoeTrail = workspace.Players.Killers.DescendantAdded:Connect(function(v)
if v.Name == "Trail" then
v.CanTouch = false
end
end)
else
AntiJohnDoeTrail:Disconnect()
for _,v in ipairs(workspace.Players.Killers:GetDescendants()) do
if v.Name == "Trail" then
v.CanTouch = true
end
end
end
end})
Anti:AddToggle("Anti John Doe Trap",{
Text = "Anti John Trail Trap",
Default = false,
Tooltip = "This is features help you cant touch with 'trap' john doe.",
Callback = function(v)
if v then
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v.Name == "Shadow" then
v.CanTouch = false
end
end
AntiJohnDoeTrap = workspace.Map.Ingame.DescendantAdded:Connect(function(v)
if v.Name == "Shadow" then
v.CanTouch = false
end
end)
else
AntiJohnDoeTrap:Disconnect()
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v.Name == "Shadow" then
v.CanTouch = true
end
end
end
end})
Anti:AddToggle("Anti Blinding",{
Text = "Anti Blinding",
Default = false,
Tooltip = "This is features help you cant 'blinding' effect.",
Callback = function(v)
if v and game:GetService("ReplicatedStorage").Modules.StatusEffects:FindFirstChild("Blinding") then
game:GetService("ReplicatedStorage").Modules.StatusEffects.Blinding.Parent = workspace
else
if workspace:FindFirstChild("Blinding") then
workspace.Blinding.Parent = game:GetService("ReplicatedStorage").Modules.StatusEffects
end
end})
